{
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [{
    "url": "heroku/ruby"
  }],
  "env": {
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "project-facemask",
  "scripts": {
    "postdeploy": "rails db:schema:load db:seed",
    "test-setup": "bin/rails assets:precompile"
  }
}